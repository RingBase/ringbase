#= require_self
#= require_tree .

@ringbase = angular.module('ringbase', ['ngRoute'])

# Sets up routing
@ringbase.config(['$routeProvider', ($routeProvider) ->
  $routeProvider.
    otherwise({
      templateUrl: "<%= asset_path('angular/templates/home.html') %>",
      controller: 'PhoneCtrl'
    }) 
])